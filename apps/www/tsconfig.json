{
	"compilerOptions": {
		"baseUrl": ".",
		"esModuleInterop": true,
		"lib": ["DOM", "DOM.Iterable", "ES2022"],
		"jsx": "react-jsx",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"paths": {
			"~/*": ["./app/*"]
		},
		"resolveJsonModule": true,
		"target": "ES2022"
	},
	"extends": ["../../tsconfig.base.json"],
	"include": ["env.d.ts", "**/*.ts", "**/*.tsx"]
}
